				<field id="volume" />
				<field id="number" />
				<field id="month" />
				<field id="issn" />
				<field id="url" />
				<field id="key" />
				<field id="note" />
			</optional>
		</type>
		<type id="Book">
			<required>
				<field id="title" />
				<field id="year" />
				<field id="publisher" />
			</required>
			<optional>
