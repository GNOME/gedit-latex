<bibtex>
	<fields>
		<field id="title" _label="Title" />
		<field id="booktitle" _label="Book Title" />
		<field id="author" _label="Author" />
		<field id="editor" _label="Editor" />
		<field id="pages" _label="Pages" />
		<field id="journal" _label="Journal" />
		<field id="volume" _label="Volume" />
		<field id="number" _label="Number" />
		<field id="month" _label="Month" />
		<field id="year" _label="Year" />
		<field id="issn" _label="ISSN" />
		<field id="series" _label="Series" />
		<field id="edition" _label="Edition" />
		<field id="publisher" _label="Publisher" />
